  </tr>

<% @infographics.each do |infographic| %>
  <tr>
    <td><%= infographic.title %></td>
    <td><%= infographic.source %></td>
    <td><%= infographic.chart_type %></td>
    <td><%= link_to 'Show', infographic %></td>
    <td><%= link_to 'Edit', edit_infographic_path(infographic) %></td>
    <td><%= link_to 'Destroy', infographic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

